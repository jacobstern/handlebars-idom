{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:compat/recommended"],
  "env": {
    "es6": true,
    "shared-node-browser": true
  },
  "rules": {
    "no-console": "warn",
    "no-var": "error",
    "no-unused-vars": "warn",
    "semi": "error",
    "no-func-assign": "off"
  }
}
