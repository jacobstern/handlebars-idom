{
  "extends": ["eslint:recommended"],
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "no-var": "warn",
    "no-unused-vars": "warn",
    "compat/compat": "off"
  }
}
