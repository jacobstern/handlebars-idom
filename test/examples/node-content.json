{
   "desc": "the compiler can insert dynamic content",
   "examples": [
      {
         "data": {
            "name": "Jake"
         },
         "desc": "in plain text",
         "template": "Hello {{ name }}!"
      },
      {
         "data": {
            "title": "A Title"
         },
         "desc": "inside an element tag",
         "template": "<h1>{{title}}</h1>"
      },
      {
         "data": {
            "content": "Hello world!",
            "firstName": "Jake",
            "lastName": "Stern"
         },
         "desc": "with nested tags",
         "template": "<div class=\"entry\">\n  <div>\n    <h1>{{firstName}} {{lastName}}</h1>\n  </div>\n  <div>{{content}}</div>\n</div>"
      }
   ]
}
